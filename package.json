{
  "name": "thyme",
  "version": "0.0.1",
  "description": "thyme",
  "scripts": {
    "postinstall": "cd ./server && npm i && cd ../client && npm i",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "s": "cd server && npm run dev",
    "c": "cd client && npm run dev",
    "debug": "cd server && npm run debug",
    "start": "cd server && npm start",
    "stop": "cd server && npm stop",
    "clean": "rm -rf server/node_modules client/node_modules",
    "build": "npm i && npm run tsc --prefix=./server && npm run build --prefix=./client",
    "build:pure": "npm run clean && npm run build"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "inquirer": "^6.0.0",
    "npm-run-all": "^4.1.3"
  },
  "keywords": [],
  "author": "browserify@qq.com",
  "license": "LGPL-3.0-or-later"
}
